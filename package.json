{
  "name": "velvet-chains-docs",
  "version": "1.0.0",
  "description": "🏴‍☠️ The most spectacular BDSM space-pirate romance git toolchain documentation",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "npm run build:lore && npm run build && npm run export",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
