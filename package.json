  "repository": {
    "type": "git",
    "url": "https://github.com/hsmalley/velvet-chains.git"
  },
  "homepage": "https://hsmalley.github.io/velvet-chains",
  "bugs": {
    "url": "https://github.com/hsmalley/velvet-chains/issues"
  },
  "engines": {
    "node": ">=20"
  }
